        },
        {
            "language": "javascript",
            "source": [
                "function birthdayCakeCandles(candles) {\n    let max = -1;\n    let count = 0;\n\n    for (let i = 0; i < candles.length; i++) {\n        if (candles[i] > max) {\n            max = candles[i];\n            count = 1;\n        } else if (candles[i] === max) {\n            count++;\n        }\n    }\n\n    return count;\n}\n"
            ],
