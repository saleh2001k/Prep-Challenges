            "language": "javascript",
            "source": [
                "function appendAndDelete(s, t, k) {\n  if (k >= s.length + t.length) {\n    return \"Yes\";\n  }\n\n  let commonLength = 0;\n\n  for (let i = 0; i < Math.min(s.length, t.length); i++) {\n    if (s[i] === t[i]) {\n      commonLength++;\n    } else {\n      break;\n    }\n  }\n\n  if (\n    (k - s.length - t.length + 2 * commonLength) % 2 === 0 &&\n    k - s.length - t.length + 2 * commonLength >= 0\n  ) {\n    return \"Yes\";\n  }\n\n  return \"No\";\n}\n"
            ],
